{
  "expo": {
    "name": "swiggy-clone",
    "slug": "swiggy-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-screen.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-screen.png"
      }
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splash-screen.png",
        "resizeMode": "cover",
        "backgroundColor": "#EF4F27"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "firebase": {
          "apiKey": "AIzaSyDK8C0w0OHSQzZ2FAnpo_iMlK0ovpJ69J8",
          "authDomain": "swiggy-clone-c857e.firebaseapp.com",
          "projectId": "swiggy-clone-c857e",
          "storageBucket": "swiggy-clone-c857e.firebasestorage.app",
          "messagingSenderId": "284357645337",
          "appId": "1:284357645337:web:04e21c757c70c8d9778688",
          "measurementId": "G-NDENCJCTDR"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "imageWidth": 200,
          "resizeMode": "fill"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "fill"
    }
  }
}
